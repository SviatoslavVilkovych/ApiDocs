      <MemberSignature Language="VB.NET" Value="Protected Overridable Sub Seed (context As TContext)" />
      <MemberSignature Language="F#" Value="abstract member Seed : 'Context -&gt; unit&#xA;override this.Seed : 'Context -&gt; unit" Usage="dbMigrationsConfiguration.Seed context" />
      <MemberType>Method</MemberType>
      <AssemblyInfo>
